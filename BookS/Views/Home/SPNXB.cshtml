@model IEnumerable<BookS.Models.SACH>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@foreach (var i in Model)
{
    <div class="col-sm-3 col-lg-3 col-md-3">
        <div class="thumbnail">
            <img src="~/image/@i.Anhbia" alt="">
            <div class="caption">
                <p>
                    <h4>
                        <a href="#">@i.TenSach</a>
                    </h4>
                </p>
                <h4 class="pull-left">$@i.GiaBan</h4>

            </div>
            <div class="ratings">
                <p class="pull-right">15 reviews</p>
                <p>
                    <span class="glyphicon glyphicon-star"></span>
                    <span class="glyphicon glyphicon-star"></span>
                    <span class="glyphicon glyphicon-star"></span>
                    <span class="glyphicon glyphicon-star"></span>
                    <span class="glyphicon glyphicon-star"></span>
                </p>
            </div>
        </div>
    </div>
}
